{
  "name": "rift-monitor-ui",
  "version": "1.0.1",
  "description": "Rift Monitor UI",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "mocha --compilers css:./test-compiler,js:babel-core/register --recursive",
    "lint": "eslint ./src ./spec"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "lodash": "^4.17.4",
    "mithril": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.26.2",
    "es6-promise": "^4.0.5",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "lost": "^8.0.0",
    "mithril-query": "2.0",
    "mocha": "^3.1.2",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.8.0",
    "postcss-extend": "^1.0.5",
    "postcss-functions": "^2.1.1",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "rucksack-css": "^0.9.1",
    "sanitize.css": "^4.1.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
